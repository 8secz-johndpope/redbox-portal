<div class="row" id="loading">
  <div class="col-xs-1">
  </div>
  <div class="col-xs-10">
    <img class="center-block" src="/images/loading.svg">
  </div>
  <div class="col-xs-1">
  </div>
</div>
<dmp-form oid="<%= oid %>" editMode="true" recordType="<%= recordType %>" ></dmp-form>
