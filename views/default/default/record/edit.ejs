<% if (!sails.config.ng2.use_bundled) { %>
<!-- Import and Load angular 2 if on dev mode-->
<script src="/dynamic/systemjs-config.js?appName=dmp"></script>
<script>
  System.import('dmp').catch(function(err){ console.error(err); });
</script>
<% } %>
